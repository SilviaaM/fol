/* ========================================================================= */
/* Interpretation of the language of graphs in the model graph1.             */
/* ========================================================================= */

sig graph1interp.

accum_sig fol, graph1model.

/* ------------------------------------------------------------------------- */
/* Term evaluation.                                                          */
/* ------------------------------------------------------------------------- */

type node0, node1, node2, node3 term.

/* ------------------------------------------------------------------------- */
/* Relations.                                                                */
/* ------------------------------------------------------------------------- */

type edge term -> term -> form.
type path term -> term -> form.

end
