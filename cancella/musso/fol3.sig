sig fol3.

/* ------------------------------------------------------------------------- */
/* Library.                                                                  */
/* ------------------------------------------------------------------------- */

type somenot (A -> o) -> (A -> o) -> o.
type all (A -> o) -> (A -> o) -> o.

/* ------------------------------------------------------------------------- */
/* Model: addition on integers modulo 3.                                     */
/* ------------------------------------------------------------------------- */

type domain int -> o.
type add3 int -> int -> int -> o.

/* ------------------------------------------------------------------------- */
/* Term datatypes.                                                           */
/* ------------------------------------------------------------------------- */

kind term type.

type zero, one, two term.
type add term -> term -> term.

/* ------------------------------------------------------------------------- */
/* Formulas.                                                                 */
/* ------------------------------------------------------------------------- */

kind form type.

type truth, false form.
type eq term -> term -> form.
type and form -> form -> form.
type forall, exists (term -> form) -> form.

/* ------------------------------------------------------------------------- */
/* Semantics.                                                                */
/* ------------------------------------------------------------------------- */

type termval term -> int -> o.

type holds form -> o.

/* ------------------------------------------------------------------------- */
/* Tests.                                                                    */
/* ------------------------------------------------------------------------- */

type test0, test1, test2, test3, test4, test5, test6, test7, test8, test9 o.

end
